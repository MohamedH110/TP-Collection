package tdd3;

import java.util.Scanner;

public class tab {
	
	static  int nmax=40;
	
	
    static void remplir(float[] t) {
       int n=t.length;
       Scanner sc = new Scanner(System.in);
        for (int i = 0; i < n; i++) {
            System.out.print("entrer une valeure" + i);
            t[i] = sc.nextFloat();
        }
    }

  
   static  float sommeelement(float p[]) {
	   float s=0;
	   for (int i=0;i<p.length;i++){
		   s=s+p[i];
	   }return s;
   }
   
   
   static float[] additionner(float t1[], float t2[]) {
	    float[] resu = new float[t1.length];
	    for (int i = 0; i < t1.length; i++) {
	        resu[i] = t1[i] + t2[i];
	    }
	    return resu;
	}

   
   
   
   
   

static float[] produit(float x,float l[]) {
	float[] resu=new float[l.length];
	for(int i=0;i<l.length;i++) {
		resu[i]=l[i]*x;
	}return resu;
}



static void lister (float k[]) {
	for(int i=0;i<k.length;i++) {
		System.out.println(k[i]);
	}
}






}
